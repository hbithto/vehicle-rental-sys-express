{
    "version": 2,
    "builds": [
        {
            "src": "dist/server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/server.js"
        }
    ],
    "crons": [
        {
            "path": "/api/v1/cron/booking-return",
            "schedule": "0 0 * * *"
        }
    ]
}